  "scripts": {
    "android": "react-native run-android --mode=developmentDebug",
    "android:release": "react-native run-android --mode=release",
    "ios": "react-native run-ios --simulator \"iPhone 15\" --scheme=BelinkHRMDev",
    "start": "react-native start --reset-cache",
    "lint": "eslint .",
    "test": "jest",
    "prettier": "prettier --write --cache --cache-location .cache/.prettier-cache src --log-level silent",
    "clean:ios": "cd ios && rm -rf Pods && rm -rf Podfile.lock && pod install",
    "clean:android": "cd android && ./gradlew clean",
    "reset": "rm -rf node_modules && npm cache clean --force && yarn install && watchman watch-del-all && rm -rf $TMPDIR/haste-map-* && rm -rf $TMPDIR/metro-cache",
    "prepare": "husky install",
    "tx": "node generate_locales.ts",
    "icons": "bash scripts/generate_icons.sh",
    "precommit": "npm run lint",
    "release:android": "cd android && ENVFILE=.env && ./gradlew assembleRelease",
    "appicon:create": "npx icon-set-creator create",
    "deploy": "bash scripts/deploy.sh",
    "deploy-code-push": "bash code_push_deploy.sh",
    "postinstall": "patch-package"
  },
